{
  "name": "Replace this world",
  "version": "1.0.0",
  "description": "Change a word you don't like to see anymore",
  "manifest_version": 2
}

  "icons": {"128":"icon.png"},
  "permissions": ["declarativeContent","storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["findText.js"]
    }
  ],
  "background": { },
  "page_action": {
    "default_icon": "128.png"
  }
}
